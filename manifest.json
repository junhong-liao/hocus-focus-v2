{
  "manifest_version": 3,
  "name": "Hocus Focus",
  "version": "1.0",
  "description": "Bolds the first two letters of every word on any webpage",
  "permissions": ["activeTab", "storage", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
